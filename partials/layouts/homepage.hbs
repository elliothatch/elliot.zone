<h1>{{title}}</h1>
<section class="store featured">
	{{!-- <h2>{{hal-link 'store' _links.store}}</h2> --}}
	<h2>store</h2>
	<div class="card-container">
		{{#each store}}
			{{> includes/card.hbs this}}
		{{/each}}
	</div>
</section>
<section class="projects featured">
	<h2>projects</h2>
	<div class="card-container">
	{{#each projects}}
		{{#unless this.hidden}}
			{{> includes/card.hbs this}}
		{{/unless}}
	{{/each}}
	</div>
</section>
<section class="code-section featured">
	<h2>code</h2>
	<ul class="items">
	{{#each code}}
		<li><a href="{{url}}">{{name}}</a> - {{about}}</li>
	{{/each}}
	</ul>
</section>
