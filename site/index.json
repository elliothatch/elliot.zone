	"code": [{
		"@type": "https://schema.org/SoftwareApplication",
		"name": "hypermedium",
		"url": "https://github.com/elliothatch/hypermedium",
		"about": "tool for creating websites (including this one!)"
		}, {
		"@type": "https://schema.org/SoftwareApplication",
		"name": "freshlog",
		"url": "https://github.com/elliothatch/freshlog",
		"about": "simple nodejs logger"
		}, {
		"@type": "https://schema.org/SoftwareApplication",
		"name": "fresh-validation",
		"url": "https://github.com/elliothatch/fresh-validation",
		"about": "runtime schema validation for typescript classes"
		}, {
		"@type": "https://schema.org/SoftwareSourceCode",
		"name": "sample code",
		"url": "https://github.com/elliothatch/sample-code",
		"about": "sample code snippets in various programming languages (for testing color schemes)"
		}, {
		"@type": "https://schema.org/SoftwareSourceCode",
		"name": "fresh-socketio-router",
		"url": "https://github.com/elliothatch/fresh-socketio-router",
		"about": "middleware router for socketio transactions"
	}],
	"_links": {
        "profile": {
            "href": "/schema/homepage"
        },
		"store": {
			"title": "store",
			"href": "/store"
		},
		"projects": {
			"title": "projects",
			"href": "/projects"
		},
		"blog": {
			"title": "blog",
			"href": "/blog"
		},
		"socialMedia": [{
			"title": "github",
			"href": "https://github.com/elliothatch/"
		}, {
			"title": "npm",
			"href": "https://www.npmjs.com/~elliothatch"
		}, {
			"title": "twitter",
			"href": "https://twitter.com/neurotischism"
		}, {
			"title": "fedi",
			"href": "https://weirder.earth/@neurotischism"
		}]
	},
	"_processors": [{
		"name": "getIndex",
		"options": {
			"property": "_links.profile.href",
			"filter": "https://schema.org/BlogPosting",
			"to": "blog"
		}
	}, {
		"name": "sort",
		"options": {
			"property": "blog",
			"key": "date-created",
			"compare": "date",
			"descending": true
		}
	}, {
		"name": "embed",
		"options": {
			"property": "blog",
			"pick": ["date-created", "description"]
		}
	}]
}
