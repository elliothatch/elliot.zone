{
	"_links": {
		"profile": {
			"href": "https://schema.org/ItemList"
		}
	},
	"@type": "https://schema.org/ItemList",
	"title": "projects",
	"itemListOrder": "Unordered",
	"_processors": [{
		"name": "copy",
		"options": {
			"uri": "/~hypermedium/dynamic/index/@type",
			"from": "index./schema/project",
			"to": "itemListElement"
		}
	}, {
			"name": "map",
			"options": {
				"processor": {
					"name": "ld:embed",
					"properties": [
						"name",
						"about",
						"thumbnailUrl"
					]
				},
				"property": "itemListElement"
			}
	}, {
		"name": "order",
		"options": {
			"property": "itemListElement",
			"key": "@id",
			"values": [
				"/projects/neurotischism",
				"/projects/mazenet",
				"/projects/sift",
				"/projects/zine-machine",
				"/projects/burgundy",
				"/projects/esp32-leds"
			]
		}
	}]
}
