{
	"_links": {
		"profile": {
			"href": "https://schema.org/AboutPage"
		}
	},
	"@type": "https://schema.org/AboutPage",

	"title": "About Me",
	"_processors": [{
		"name": "copyFile",
		"options": {
			"uri": "/about/post.md",
			"to": "markdown"
		}
	}, {
		"name": "markdown",
		"options": {
			"from": "markdown",
			"to": "body"
		}
	}]
}
